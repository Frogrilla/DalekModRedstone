{
  "multipart": [
    {
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_base"}
    },

    {
      "when": {"facing": "north"},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_emitter"}
    },
    {
      "when": {"facing": "east"},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_emitter", "y": 90}
    },
    {
      "when": {"facing": "south"},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_emitter", "y": 180}
    },
    {
      "when": {"facing": "west"},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_emitter", "y": 270}
    },
    {
      "when": {"facing": "up"},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_emitter", "x": 270}
    },
    {
      "when": {"facing": "down"},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_emitter", "x": 90}
    },

    {
      "when": {"north": true},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_segment"}
    },
    {
      "when": {"east": true},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_segment", "y": 90}
    },
    {
      "when": {"south": true},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_segment", "y": 180}
    },
    {
      "when": {"west": true},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_segment", "y": 270}
    },
    {
      "when": {"up": true},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_segment", "x": 270}
    },
    {
      "when": {"down": true},
      "apply": {"model": "dalek_mod_redstone:block/sonic_adder/sonic_adder_segment", "x": 90}
    }
  ]
}