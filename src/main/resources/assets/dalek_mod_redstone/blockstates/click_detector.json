{
  "multipart":[
    {
      "when": {"facing": "north"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/click_detector",
        "y": 0
      }
    },
    {
      "when": {"facing": "east"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/click_detector",
        "y": 90
      }
    },
    {
      "when": {"facing": "south"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/click_detector",
        "y": 180
      }
    },
    {
      "when": {"facing": "west"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/click_detector",
        "y": 270
      }
    },

    {
      "when": {"activated":  true, "facing": "north"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/note",
        "y": 0
      }
    },
    {
      "when": {"activated":  true, "facing": "east"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/note",
        "y": 90
      }
    },
    {
      "when": {"activated":  true, "facing": "south"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/note",
        "y": 180
      }
    },
    {
      "when": {"activated":  true, "facing": "west"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/note",
        "y": 270
      }
    },

    {
      "when": {"pulse": 0, "facing": "north"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/slider",
        "y": 0
      }
    },
    {
      "when": {"pulse": 0, "facing": "east"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/slider",
        "y": 90
      }
    },
    {
      "when": {"pulse": 0, "facing": "south"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/slider",
        "y": 180
      }
    },
    {
      "when": {"pulse": 0, "facing": "west"},
      "apply":{
        "model": "dalek_mod_redstone:block/click_detector/slider",
        "y": 270
      }
    }
  ]
}